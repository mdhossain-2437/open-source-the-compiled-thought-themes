    "commands": [
      {
        "command": "delowar.selectTheme",
        "title": "TCT: Select Theme"
      },
      {
        "command": "delowar.toggleItalic",
        "title": "TCT: Toggle Italic Variant"
      },
      {
        "command": "delowar.enableAutoTheme",
        "title": "TCT: Enable Auto Theme Switching"
      },
      {
        "command": "delowar.previewTheme",
        "title": "TCT: Preview Themes"
      }
    ],
    "configuration": {
      "title": "TCT Extension Settings",
      "properties": {
        "delowar.enableItalic": {
          "type": "boolean",
          "default": true,
          "description": "Enable italic variants for themes that support it"
        },
        "delowar.autoTheme": {
          "type": "boolean",
          "default": false,
          "description": "Automatically switch themes based on time and context"
        },
        "delowar.fontFamily": {
          "type": "string",
          "default": "Operator Mono, Fira Code, JetBrains Mono, Cascadia Code, Consolas, monospace",
          "description": "Optimized font family for coding"
        },
        "delowar.fontLigatures": {
          "type": "boolean",
          "default": true,
          "description": "Enable font ligatures for better code readability"
        },
        "delowar.smartRecommendations": {
          "type": "boolean",
          "default": true,
          "description": "Enable smart theme recommendations based on file type and time"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish",
    "clean": "rimraf out",
    "precompile": "npm run clean"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
