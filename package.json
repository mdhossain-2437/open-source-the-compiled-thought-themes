      },
      {
        "command": "DelowarHossain.customizeKeyboardShortcuts",
        "title": "TCT: Customize Theme Shortcuts"
      }
    ],
    "configuration": {
      "title": "TCT Extension Settings",
      "properties": {
        "DelowarHossain.enableItalic": {
          "type": "boolean",
          "default": true,
          "description": "Enable italic variants for themes that support it"
        },
        "DelowarHossain.autoTheme": {
          "type": "boolean",
          "default": false,
          "description": "Automatically switch themes based on time and context"
        },
        "DelowarHossain.fontFamily": {
          "type": "string",
          "default": "Operator Mono, Fira Code, JetBrains Mono, Cascadia Code, Consolas, monospace",
          "description": "Optimized font family for coding"
        },
        "DelowarHossain.fontLigatures": {
          "type": "boolean",
          "default": true,
          "description": "Enable font ligatures for better code readability"
        },
        "DelowarHossain.smartRecommendations": {
          "type": "boolean",
          "default": true,
          "description": "Enable smart theme recommendations based on file type and time"
        },
        "tct.ai.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI-powered features using Gemini"
        },
        "tct.ai.suggestions": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI code suggestions and theme recommendations"
        },
        "tct.ai.codeAnalysis": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI code analysis for bug detection"
        },
        "tct.themes.customization": {
          "type": "object",
          "default": {},
          "description": "Store custom theme configurations"
        },
        "tct.analytics.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable theme usage analytics to improve recommendations"
        },
        "tct.community.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable community features like theme sharing and ratings"
        },
        "tct.scheduling.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable theme scheduling based on time zones"
        },
        "tct.scheduling.timeZone": {
          "type": "string",
          "default": "auto",
          "description": "Time zone for theme scheduling (auto or IANA time zone name)"
        },
        "tct.iconPacks": {
          "type": "object",
          "default": {
            "selected": "material-design",
            "custom": {}
          },
          "description": "Icon pack configuration and custom icon mappings"
        },
        "tct.workspace.themeOverrides": {
          "type": "object",
          "default": {},
          "description": "Workspace-specific theme settings and overrides"
        },
        "tct.keyboardShortcuts": {
          "type": "object",
          "default": {},
          "description": "Custom keyboard shortcuts for theme actions"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "node ./scripts/version.js && vsce package --no-dependencies",
    "publish": "node ./scripts/version.js && vsce publish",
    "release": "node ./scripts/release.js",
    "clean": "rimraf out",
    "precompile": "npm run clean",
    "version:bump": "node ./scripts/version.js bump",
    "changelog": "node ./scripts/version.js changelog"
  },
