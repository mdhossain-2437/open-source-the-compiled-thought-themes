{
  "name": "TCT Sunset",
  "type": "dark",
  "colors": {
    "editor.background": "#331f27",
    "editor.foreground": "#fddac7",
    "editorLineNumber.foreground": "#fc9776",
    "editor.selectionBackground": "#fa5a2855",
    "editor.inactiveSelectionBackground": "#fa5a2822",
    "editorCursor.foreground": "#ffae00",

    "activityBar.background": "#44242c",
    "sideBar.background": "#44242c",
    "statusBar.background": "#44242c",
    "terminal.foreground": "#fed7aa",
    "titleBar.activeBackground": "#5a2d39",
    "tab.activeBackground": "#5a2d39",

    "editorBracketHighlight.foreground1": "#e06c75",
    "editorBracketHighlight.foreground2": "#d19a66",
    "editorBracketHighlight.foreground3": "#e5c07b",
    "editorBracketHighlight.foreground4": "#98c379",
    "editorBracketHighlight.foreground5": "#56b6c2",
    "editorBracketHighlight.foreground6": "#c678dd"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ebb09f"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#ffd2b1"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#fc9776",
        "fontStyle": ""
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#ffc18e"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#fda67e"
      }
    }
  ]
}
